<p id="notice"><%= notice %></p>

<h1>Harvest Procedures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @harvest_procedures.each do |harvest_procedure| %>
      <tr>
        <td><%= harvest_procedure.name %></td>
        <td><%= link_to 'Show', harvest_procedure %></td>
        <td><%= link_to 'Edit', edit_harvest_procedure_path(harvest_procedure) %></td>
        <td><%= link_to 'Destroy', harvest_procedure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Harvest Procedure', new_harvest_procedure_path %>
