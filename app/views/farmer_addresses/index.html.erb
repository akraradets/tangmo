<p id="notice"><%= notice %></p>

<h1>Farmer Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Farmer</th>
      <th>Houseno</th>
      <th>Moo</th>
      <th>Tambon</th>
      <th>Amphoe</th>
      <th>Province</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farmer_addresses.each do |farmer_address| %>
      <tr>
        <td><%= farmer_address.farmer %></td>
        <td><%= farmer_address.houseNo %></td>
        <td><%= farmer_address.moo %></td>
        <td><%= farmer_address.tambon %></td>
        <td><%= farmer_address.amphoe %></td>
        <td><%= farmer_address.province %></td>
        <td><%= farmer_address.zipcode %></td>
        <td><%= link_to 'Show', farmer_address %></td>
        <td><%= link_to 'Edit', edit_farmer_address_path(farmer_address) %></td>
        <td><%= link_to 'Destroy', farmer_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Farmer Address', new_farmer_address_path %>
